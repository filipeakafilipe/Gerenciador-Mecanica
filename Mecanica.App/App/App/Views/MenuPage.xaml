<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="App.Views.MenuPage"
             Title="{Binding Title}">

    
    <StackLayout>

            <Label></Label>
            <Button Text="Cadastrar Usuário" Command="{Binding CriarPerfilCommand}" BackgroundColor="#2296f3" TextColor="White"/>
            <Button Text="Alterar Usuário" Command="{}" BackgroundColor="#2296f3" TextColor="White"/>
            <Button Text="Cadastrar Veículo Usuário" Command="{}" BackgroundColor="#2296f3" TextColor="White"/>
            <Button Text="Alterar Veículo" Command="{}" BackgroundColor="#2296f3" TextColor="White"/>
            <Button Text="Cadastrar Pedido" Command="{}" BackgroundColor="#2296f3" TextColor="White"/>
            <Button Text="Alterar Pedido" Command="{}" BackgroundColor="#2296f3" TextColor="White"/>
            <Button Text="Cadastrar Tipo de Serviço" Command="{}" BackgroundColor="#2296f3" TextColor="White"/>
            <Button Text="Alterar Tipo de Serviço" Command="{}" BackgroundColor="#2296f3" TextColor="White"/>

            

       
    </StackLayout>
    

</ContentPage>